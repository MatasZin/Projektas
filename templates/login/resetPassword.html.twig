{% extends 'base.html.twig' %}

{% block content %}

    {% for message in app.flashes('warning') %}
        <div class="flashes-warning">{{ message }} </div>
    {% endfor %}

    {{ form_start(form) }}
    {{ form_errors(form) }}
    {{ form_widget(form) }}
    {{ form_end(form) }}

{% endblock %}