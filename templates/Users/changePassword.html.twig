{% extends 'base.html.twig' %}

{% block content %}

    {% for message in app.flashes('successful') %}
        <div class="flashes-successful">{{ message }}</div>
    {% endfor %}

    {{ form_start(form) }}
    {{ form_errors(form) }}
    {{ form_widget(form) }}
    {{ form_end(form) }}

    <a href="{{ path('myProfile') }}" class="modern" style="margin-top: 10px;">Back</a>

{% endblock %}